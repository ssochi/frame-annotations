<el-upload class="upload-demo" action="" :on-preview="${handle_preview}" :on-remove="${handle_remove}" :file-list="${file_list}.group" list-type="picture">
     <el-button size="small" type="primary" v-if="${file_list}.group.length <= 0">
      点击上传
     </el-button>
     <div slot="tip" class="el-upload__tip">
      只能上传jpg/png文件
     </div>
    </el-upload>
    <el-dialog :visible.sync="${dialog_visible}">
     <img width="100%" :src="${dialog_image}">
</el-dialog>